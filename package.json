{
  "name": "mbtiles-stacker",
  "description": "Tiny raster+vector tiles and static file server.",
  "version": "1.5.0",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "scripts": {
    "start": "micro",
    "debug": "nodemon -r esm --inspect-brk --inspect=0.0.0.0:9229 index.js --port 8000",
    "commit": "npx git-cz",
    "report-coverage": "coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@mapbox/vector-tile": "^1.3.1",
    "better-sqlite3": "^5.4.3",
    "canvas": "^2.6.1",
    "jimp": "^0.9.3",
    "log-less-fancy": "^1.4.0",
    "micro": "^9.3.4",
    "node-fetch": "^2.6.0",
    "pbf": "^3.2.1",
    "zlib": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Artsdatabanken/mbtiles-stacker.git"
  },
  "keywords": [
    "mbtiles",
    "raster-tiles",
    "vector-tiles"
  ],
  "author": "Bjorn Reppen <bjreppen@gmail.com> (https://github.com/bjornreppen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Artsdatabanken/mbtiles-stacker/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "homepage": "https://github.com/Artsdatabanken/mbtiles-stacker#readme"
}
